.palette {
	position: absolute;
	left: 980px;
	background-color: #FFF;
	width: 150px;
	height: 121px;
	border-radius: 5px;
	padding: 9px;
	box-shadow: 1px 2px 7px rgba(0, 0, 0, 0.5);
	border: 1px solid gray;
	visibility: hidden;
	z-index: 20;
}

.palette:before {
	content: '';
	position: absolute;
	top: 9px;
	left: 29px;
	width: 0px;
	height: 0px;
	border: 12px solid transparent;
	border-bottom-color: black;
	border-top: 0;
	margin-left: -21px;
	margin-top: -21px;
}

.palette:after {
	content: '';
	position: absolute;
	top: 9px;
	left: 29px;
	width: 0px;
	height: 0px;
	border: 11px solid transparent;
	border-bottom-color: #FFF;
	border-top: 0;
	margin-left: -20px;
	margin-top: -20px;
	z-index: 20;
}

