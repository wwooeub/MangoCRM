.table_txt{
   display: inline-block;
    vertical-align: middle;
    width: 100%;
    height: 35px;
    background: #fff;
    border: 1px solid #eee;
    border-radius: 4px;
    box-sizing: border-box;
    font-size: 14px;
    outline: none;
}
.table_txt:focus{
   border: 1px solid #F2B807;
    box-shadow: none;
    outline: none;
   
}
