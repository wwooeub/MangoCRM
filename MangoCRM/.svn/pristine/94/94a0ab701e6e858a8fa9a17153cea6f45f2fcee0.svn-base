@charset "UTF-8";
.title_area {
	text-indent: 20px;
	padding: 20px 0px;
	font-size: 20pt;
	font-weight: bold;
	color: #595959;
}
.content_area {
	width: 1200px;
	font-size: 14pt;
}
.btn_area {
   width: 1080px;
   height: 50px;
}
.next_Btn {
   display: table;
   width : 80px;
   height: 30px;
   font-size: 10pt;
   color: #595959;
   text-align : center;
   border-radius : 4px;
   float : right;
   background-color : #F2B807;
   padding-top : 7px;
   margin-top : 10px;
   margin-left : 10px;
}
.next_Btn:hover {
   background-color : #F2CB07;
   cursor : pointer;
}
.circle{
display:inline-block;
width:30px;
height:30px;
border-radius:50%;
background:#595959;
font-size:16px;
color:white;
text-align:center;  
line-height:30px;
font-weight: bold;  
}
