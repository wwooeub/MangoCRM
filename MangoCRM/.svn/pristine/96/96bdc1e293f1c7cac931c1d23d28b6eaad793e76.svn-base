				html +="				<td>담당자</td>                                                                                                  ";
				html +="			</tr>                                                                                                                ";
				html +="			</thead>";
				html +="			<tbody>";
				if(list.length > 0) {
					for(var i in list) {
						html += "<tr class=\"pop_column\" name=\"" + list[i].NO+","+list[i].NAME + "\">";
		          		html += "<td>" + list[i].DEPART_NAME + "</td>";
