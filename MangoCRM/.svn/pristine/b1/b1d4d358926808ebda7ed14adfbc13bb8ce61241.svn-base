@charset "UTF-8";
html, body {
	margin: 0;
	font-size: 0pt;
	width: 100%;
	height: 100%;
}
* {
	box-sizing: border-box;
	font-family: 'Gothic A1', sans-serif;
}
.left_wrap {
	display: inline-block;
	vertical-align: top;
	width: 200px;
	height: 100%;
	background-color: #595959;
	box-shadow: 1px 0px 3px rgba(0, 0, 0, 0.5);
}

.right_wrap {
	display: inline-block;
	vertical-align: top;
	width: calc(100% - 200px);
	height: 100%;
}

.logo_wrap {
	display: table;
	width: 100%;
	height: 60px;
	background-color: #262626;
	font-size: 20px;
	font-weight: bold;
	color: #F2F2F2;
}

.logo_wrap > div {
	display: table-cell;
	vertical-align: middle;
	width: 100%;
	text-align: center;
	text-indent: -10px;
}

.logo_wrap > div > img {
	width: 25px;
	height: 25px;
	vertical-align: middle;
}

.logo_wrap > div > span {
	margin-left: 5px;
	color: #F2CB07;
}

.menu_wrap {
	font-weight: bold;
	text-indent: 10px;
	cursor: pointer;
}

.first_menu, .first_menu_on {
	display: table;
	width: 100%;
	font-size: 14pt;
	color: #F2F2F2;
	min-height: 50px;
}

.first_menu:hover {
	background-color: #F2B807;
	color: #595959;
}

.first_menu_on {
	background-color: #F2B807;
	color: #595959;
}

.first_menu > div, .first_menu_on > div {
	display: table-cell;
	vertical-align: middle;
	width: 100%;
}

.menu_txt {
	display: inline-block;
	vertical-align: top;
	width: calc(100% - 50px);
	text-indent: 0px;
}

.first_menu_wrap .first_menu .menu_gbn {
	display: inline-block;
	vertical-align: top;
	width: 25px;
	height: 25px;
	background-image: url('../images/common/close1.png');
	background-size: 20px;
	background-repeat: no-repeat;
}

.first_menu_wrap .first_menu_on .menu_gbn {
	display: inline-block;
	vertical-align: top;
	width: 25px;
	height: 25px;
	background-image: url('../images/common/open1.png');
	background-size: 20px;
	background-repeat: no-repeat;
}

.second_menu, .second_menu_on {
	display: table;
	width: 100%;
	font-size: 12pt;
	color: #F2F2F2;
	height: 40px;
	box-shadow: 0px 1px 3px rgba(0, 0, 0, 0.5);
}

.second_menu:hover {
	background-color: #F2CB07;
	color: #595959;
}

.second_menu_on {
	background-color: #F2CB07;
	color: #595959;
}

.second_menu > div, .second_menu_on > div {
	display: table-cell;
	vertical-align: middle;
	width: 100%;
}

