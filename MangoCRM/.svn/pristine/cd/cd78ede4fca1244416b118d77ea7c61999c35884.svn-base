.attachUpload {
   	display: none;
}

.writeBtn {
	display: inline-block;
	float:right;
	font-size:13pt;
}
.writeBtn > div {
	text-align: center;
	display: inline-block;
	vertical-align: middle;
	width:120px;
	padding-top:10px;
	height: 40px;
	background-color: #F2B807;
	cursor: pointer;
	
}

.writeBtn :hover{
	background-color: #F2B807;
	color: #F2F2F2;
}

.btn_area {
	width: 1080px;
	height: 50px;
}
.save_Btn, .next_Btn, .back_Btn {
   display: table;
   width : 80px;
   height: 30px;
   font-size: 10pt;
   color: #595959;
   text-align : center;
   border-radius : 5px;
   float : right;
   background-color : #F2B807;
   padding-top : 7px;
   margin-top : 10px;
   margin-left : 10px;
}
.save_Btn:hover, .next_Btn:hover, .back_Btn:hover {
   background-color : #F2CB07;
   cursor : pointer;
}

.circle{
	display:inline-block;
	width:30px;
	height:30px;
	border-radius:50%;
	background:#595959;
	font-size:16px;
	color:white;
	text-align:center;  
	line-height:30px;
	font-weight: bold;  
}

.circle1{
	display:inline-block;
	width:30px;
	height:30px;
	border-radius:50%;
	background:#F2CB07;
	font-size:16px;
	color:black;
