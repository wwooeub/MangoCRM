/* mms_area */
.mms_area{
	
	width: 400px;
	height: 470px;
	border: 2px solid #F2B807;
	text-align:center;
}

.mms_title{
	padding-top: 10px;
	height:30px;
	text-align:center;
}

.mms_content{
	height: 350px;
	width: 300px;
}

.mms_number{
	display:inline-block;
	height: 50px;
	width:300px;
	font-size:13pt;
}

.PH{
	height: 40px;
	width:300px;
	float:left;
}

.file_add_input {
	height: 40px;
	width: 240px;
	float: left;
}

.mms_bottom_area {
	display:inline-block;
	width: 300px;
}

.mms_btn{
	display: inline-block;
	text-align: center;
	height: 50px;
	width: 50px;
	float: right;
}
.mms_btn>div{
	display:inline-block;
	height: 40px;
	width:50px;
	font-size:10pt;
	float:right;
	background-color : #F2B807;
	padding-top:10px;
	font-weight: bold;
	text-align:center;
	border:none;
	border-radius:5px;
}
.mms_btn>div:hover{
	cursor:pointer;
	background-color:#F2CB07;
	color:#595959;
}

