table{
   width:1200px;
   border-collapse : collapse;
   padding : 0px;
   margin : 0px;
   border-spacing : 0px;
   font-size : 10pt;
   border-top : 2px solid #F2CB07;
}

td{
   padding : 10px 15px;
   font-weight : bold;
   text-align:center;
}
.write_btn {
    display: inline-block;
    width: 1200px;
    height: 50px;
    vertical-align: top;
}
.write_btn > div {
   display: table;
   width : 80px;
   height: 30px;
   font-size: 10pt;
   color: #595959;
   text-align : center;
   border-radius : 5px;
	float : right; 
   background-color : #F2B807;
   padding-top : 7px;
   margin-top : 20px;
   margin-left : 10px;
}
/*버튼의 hover입니다.*/
.write_btn > div:hover {
   background-color : #F2CB07;
   cursor : pointer;
}

