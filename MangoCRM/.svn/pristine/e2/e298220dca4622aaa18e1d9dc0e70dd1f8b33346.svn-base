@charset "UTF-8";
.msg {
	display : inline-block;
	width : 1080px;
	min-width : 880px;
	height : 730px;
	background-color : #fff7cc;
}

.top_bar {
	display : inline-block;
	width : 1080px;
	height : 50px;
	background-color : #ffe24c;
}

.actionForm {
	display: inline-block;
	vertical-align: top;
}

.top_bar_left {
	display : inline-block;
	width : 680px;
	height : 50px;
	background-color : #ffe24c;
	vertical-align: top;
}

.top_bar_right {
	display : inline-block;
	width : 395px;
	height : 50px;
	background-color : #ffe24c;
}

.top_bar_right > div {
   display: table;
   width : 120px;
   height: 25px;
   font-size: 10pt;
   color: #595959;
   text-align : center;
   border-radius : 5px;
   float : right;
   background-color : #F2B807;
   padding-top : 5px;
   margin-top : 10px;
   margin-left : 10px;
}

.top_bar_right > div:hover {
   background-color : #F2CB07;
   cursor : pointer;
}

.srch{
	display : inline-block;
	width : 200px;
	height : 40px;
	margin-top : 10px;
	margin-left : 50px;
}

.srch_input {
	display : inline-block;
	width : 120px;
	height : 22px;
	margin-top: 3px;
}


.srch_btn { /* 그림 크기 조정하는 방법 = ? */
	display : inline-block;
	/* background-image : url('./images/out/serch-btn.png'); */
	/* background-repeat : no-repeat; */
	width : 60px;
	height : 35px;
	background-color : #595959;
	color : #f2f2f2;
	vertical-align: top;
	border : none;
	border-radius : 5px;
	cursor: pointer;
	margin-left: 7px;
}

.srch_btn:hover {
	background-color : #F2CB07;
	color : #595959;
}

